{
    "name": "discord-profanity-filter-bot",
    "version": "1.0.0",
    "main": "dist/index.js",
    "type": "commonjs",
    "watch": [
        "*.ts"
    ],
    "ext": "ts",
    "exec": "ts-node index.ts",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "nodemon --watch './**/*.ts' --exec 'ts-node' index.ts"
    },
    "dependencies": {
        "@google/genai": "^0.12.0",
        "axios": "^1.6.8",
        "discord.js": "^14.14.1",
        "dotenv": "^16.4.5"
    },
    "devDependencies": {
        "@types/node": "^20.11.30",
        "@types/ws": "^8.5.10",
        "nodemon": "^3.1.10",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5"
    }
}
